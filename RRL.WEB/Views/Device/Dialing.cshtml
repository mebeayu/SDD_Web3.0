
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>拨打电话</title>
    <style>
        a {
            -webkit-tap-highlight-color: rgba(255,0,0,0);
        }
    </style>
     
</head>
<body style='margin:0;'>
    <p>
        <span style="font-size:16px;"><strong>点击拨打电话:<a href="tel:@Request.QueryString["tel"]">@Request.QueryString["tel"]</a></strong></span>
    </p>
    <p>
        <span style="font-size:16px;margin-left:10px;"><span style="line-height:2;"><a href="/#/index">[点击返回商城]</a></span></span><br />
    </p>
</body>
</html>
