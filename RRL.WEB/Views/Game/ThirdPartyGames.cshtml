@{
Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title></title>
    <style>
        * {
            margin: 0;
            padding: 0;
        }

        body {
            position: absolute;
            width: 100%;
            height: 100%;
        }

        .wj-bot-iframe {
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            width: 100%;
            height: 100%;
            overflow-y: auto;

        }

        .wj-bot-iframe iframe {
            display: block;
            width: 100%;
            height: 100%;
            background: #eee;
        }

        .gametop {
            position: absolute;
            height: 50px;
            z-index: 2;
        }

        .gametop .gamenav, .gametop .gamenav div {
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
            overflow: hidden;
        }

        .gametop .gamenav {
            -webkit-box-pack: justify;
            -ms-flex-pack: justify;
            justify-content: space-between;
            padding: 0 .4rem;
            background: rgba(255, 255, 255, .8);
            font-size: 32px;
            line-height: 1.17333333rem;
            overflow: hidden;
            border-bottom-right-radius: 10px;
            box-shadow: 2px 4px 3px rgb(0, 0, 0, .2);
        }

        .gametop .gamenav img {
            width: .53333333rem;
            padding-right: 6px;
        }

        .gamenav div {

            line-height: 50px;
            font-size: 18px;
            color: #444;
        }
    </style>
</head>
<body>
